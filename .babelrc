{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["node 9"]
        },
        "debug": false,
        "modules": "commonjs",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "helpers": true,
        "regenerator": false,
        "useESModules": true
      }
    ]
  ]
}